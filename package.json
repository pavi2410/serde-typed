{
  "name": "serde-typed",
  "version": "0.0.1",
  "description": "Pure TypeScript serialization library inspired by Rust's serde",
  "keywords": [
    "typescript",
    "serialization",
    "deserialization",
    "serde",
    "json",
    "transform"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./safe": {
      "types": "./dist/safe.d.ts",
      "import": "./dist/safe.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "lint": "biome check src tests",
    "format": "biome format --write src tests",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build && pnpm test"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@10.13.1",
  "devDependencies": {
    "@biomejs/biome": "^2.1.4",
    "@types/node": "^24.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "typescript": "^5.9.2",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pavi2410/serde-typed"
  },
  "bugs": {
    "url": "https://github.com/pavi2410/serde-typed/issues"
  },
  "homepage": "https://github.com/pavi2410/serde-typed#readme",
  "author": "Your Name",
  "license": "MIT"
}
