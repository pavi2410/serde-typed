{
  "name": "@rustify/serde",
  "version": "0.0.2",
  "description": "Pure TypeScript serialization library inspired by Rust's serde",
  "keywords": [
    "typescript",
    "serialization",
    "deserialization",
    "serde",
    "json",
    "transform"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "lint": "biome check src tests examples",
    "format": "biome format --write src tests examples",
    "typecheck": "tsc --noEmit",
    "examples": "pnpm run build && pnpm run examples:basic && pnpm run examples:recursive",
    "examples:basic": "tsx examples/basic-usage.ts",
    "examples:recursive": "tsx examples/recursive-types.ts",
    "prepublishOnly": "pnpm run build && pnpm test"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@10.13.1",
  "devDependencies": {
    "@biomejs/biome": "^2.1.4",
    "@types/node": "^24.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vite": "^7.1.2",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rustify-ts/serde"
  },
  "bugs": {
    "url": "https://github.com/rustify-ts/serde/issues"
  },
  "homepage": "https://github.com/rustify-ts/serde#readme",
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@rustify/result": "^0.0.1"
  }
}
